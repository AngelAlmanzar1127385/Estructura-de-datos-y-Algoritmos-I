#include <iostream>
using namespace std;

bool esPrimo(int x) {
    if (x < 2) return false;
    if (x == 2) return true;
    if (x % 2 == 0) return false;
    int lim = sqrt(x);
    for (int i = 3; i <= lim; i += 2)
        if (x % i == 0) return false;
    return true;
}

int main() {
    int n;
    cout << "Ingrese n: ";
    if (!(cin >> n) || n <= 0) return 0;

    int encontrados = 0, posible = 2;
    while (encontrados < n) {
        if (esPrimo(posible)) {
            cout << posible << (encontrados + 1 == n ? '\n' : ' ');
            encontrados++;
        }
        posible++;
    }
    return 0;
}
